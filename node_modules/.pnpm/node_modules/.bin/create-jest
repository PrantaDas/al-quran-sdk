#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pranta/Workspace/al-quran-api/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.14.12_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4_/node_modules/create-jest/bin/node_modules:/home/pranta/Workspace/al-quran-api/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.14.12_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4_/node_modules/create-jest/node_modules:/home/pranta/Workspace/al-quran-api/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.14.12_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4_/node_modules:/home/pranta/Workspace/al-quran-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pranta/Workspace/al-quran-api/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.14.12_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4_/node_modules/create-jest/bin/node_modules:/home/pranta/Workspace/al-quran-api/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.14.12_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4_/node_modules/create-jest/node_modules:/home/pranta/Workspace/al-quran-api/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.14.12_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4_/node_modules:/home/pranta/Workspace/al-quran-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../create-jest/bin/create-jest.js" "$@"
else
  exec node  "$basedir/../create-jest/bin/create-jest.js" "$@"
fi
